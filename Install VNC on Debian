sudo apt update
sudo apt install -y xfce4 xfce4-goodies tigervncserver dbus-x11
vncpasswd
vncserver :1
vncserver -kill :1
echo '#!/bin/bash' > ~/.vnc/xstartup
echo 'unset DBUS_SESSION_BUS_ADDRESS' >> ~/.vnc/xstartup
echo 'startxfce4' >> ~/.vnc/xstartup
chmod +x ~/.vnc/xstartup
vncserver :1
sudo dpkg -i goo*.deb
